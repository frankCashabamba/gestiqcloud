# Root .env (used by docker compose for interpolation only)

# Database DSN for services INSIDE compose (host is the service name 'db')
DB_DSN=postgresql://postgres:root@db:5432/gestiqclouddb_dev

# Toggle imports module + migrations
IMPORTS_ENABLED=1
RUN_MIGRATIONS=1

# Backend URL expected by Admin in LOCAL (compose override may also set it)
FRONTEND_URL=http://localhost:8081

# Redis (worker profile)
REDIS_URL=redis://redis:6379/0

# Render (prod) migrate job config (set only in deployment environment)
# RENDER_MIGRATE_JOB_ID=
# RENDER_API_TOKEN=

