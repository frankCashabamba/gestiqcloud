{
  "name": "admin-app",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "postbuild": "node -e \"const fs=require('fs'); ['200.html','404.html'].forEach(f=>fs.copyFileSync('dist/index.html', 'dist/'+f)); console.log('Copied 200.html and 404.html');\"",
    "preview": "vite preview --port 4173",
    "typecheck": "tsc -p tsconfig.node.json --noEmit && tsc -p tsconfig.json --noEmit",
    "check": "npm run typecheck",
    "test": "vitest run"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "idb-keyval": "^6.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.0",
    "react-toastify": "^11.0.5"
  },
  "devDependencies": {
    "@types/node": "^20.19.11",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vite-pwa/assets-generator": "^0.2.6",
    "@vitejs/plugin-react": "^4.2.0",
    "typescript": "^5.9.2",
    "vite": "^5.2.0",
    "vite-plugin-pwa": "^0.20.0",
    "vitest": "^2.1.4"
  }
}

