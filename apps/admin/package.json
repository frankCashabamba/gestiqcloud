    "idb-keyval": "^6.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.0",
    "react-toastify": "^11.0.5"
  },
  "devDependencies": {
    "@types/node": "^20.19.11",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vite-pwa/assets-generator": "^0.2.6",
    "@vitejs/plugin-react": "^4.2.0",
    "typescript": "^5.9.2",
    "vite": "^5.2.0",
    "vite-plugin-pwa": "^0.20.0"
  }
}

