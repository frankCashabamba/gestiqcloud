{
  "name": "admin-app",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "postbuild": "cp dist/index.html dist/200.html || copy dist/index.html dist/200.html && (cp dist/index.html dist/404.html || copy dist/index.html dist/404.html)",
    "preview": "vite preview --port 4173",
