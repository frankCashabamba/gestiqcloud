    "i18next-browser-languagedetector": "^8.2.0",
    "idb-keyval": "^6.2.1",
    "jsbarcode": "^3.12.1",
    "lucide-react": "^0.451.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^16.5.3",
